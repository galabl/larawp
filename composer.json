{
    "name": "galabl/larawp",
    "description": "LaraWP WordPress plugin template",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragan Galic",
            "email": "galicdragan6@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "galabl/larawp-framework": "^0.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "Galabl\\LaraWp\\LaraWpInstaller::postCreateProject"
        ]
    },
    "autoload": {
        "psr-4": {
            "Galabl\\LaraWp\\": "src/"
        }
    }
}
